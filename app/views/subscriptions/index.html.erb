<div class="gooi-container">
  <div class="outline">
    <div>
      <div class="d-flex">
        <%= link_to "Today's Subscriptions", today_subscriptions_path, class: "outline" %>
      </div>
      <br>
      <h1>All Subscriptions</h1>
      <% @subscriptions.each do |subscription| %>
        <p> <%= subscription.customer_id %> - <%= link_to subscription.user.first_name, subscription_path(subscription) %> - <%= subscription.start_date&.strftime('%e %B %Y') %></p>
        <%#= link_to "log collection", new_subscription_collection_path(subscription) %>
      <% end %>
    </div>
  </div>
</div>
